<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Pressed state -->
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#65000000" /> <!-- Darker background color for pressed state -->
            <corners android:radius="2000dp" /> <!-- You can adjust the corner radius as needed -->
        </shape>
    </item>
</selector>
